{
  "version": "4.0.0-*",
  "commands": {
    "Tryouts": "Tryouts"
  },

  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "emitEntryPoint": true,
    "compile": {
      "include": [ "**/*.cs" ]
    }
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
      },
      "imports": "dnxcore50"
    }
  },

  "dependencies": {
    "Sparrow": "4.0.0-*",
    "AsyncFriendlyStackTrace": "1.4.1",
    "FastTests": "4.0.0-*",
    "Microsoft.CSharp": "4.3.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "Raven.Client": "4.0.0-*",
    "Raven.Server": "4.0.0-*",
    "SlowTests": "4.0.0-*",
    "Voron": "4.0.0-*"
  }
}
